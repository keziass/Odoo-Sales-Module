            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data>
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Customer</field>
            <field name="code">res.partner</field>
            <field name="prefix">CST</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
