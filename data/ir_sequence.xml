<odoo>
    <data>
        <record id="seq_product" model="ir.sequence">
            <field name="name">Discount Product</field>
            <field name="code">swalayan_sales.disc_pro</field>
            <field name="prefix">DP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
